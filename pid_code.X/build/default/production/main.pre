detected|=0b00000001;
if(RA7==0)
detected|=0b00000010 ;
if(RC0==0)
detected|=0b00000100;
if(RC1==0)
detected|= 0b00001000;
if(RD0==0)
detected|= 0b00010000;

}
if(detected==0b00010101)
{
sw = 2;
return(0);
}
else if(detected==0b00000001)
{
return(1);
}
else if(detected==0b00000010)
