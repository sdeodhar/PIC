void pid(void)
{

pid_error();
if(pid_cycle_no>1)
{
u=(u1 + (k1*e0) + (k2*e1) + (k3*e2));

if (u > 1000.0) u = 1000.0;
if (u < 0.0) u = 0.0;

u1 = u;

outint = u * 1 ;
if ( outint > 1000 )
{
outint = 1000 ;
}

if ( outint < 0 )
{
outint = 0 ;
}


}
}

void pid_error(void)
{
switch(pid_cycle_no)
{
case 0: e2=calc_err();
pid_cycle_no++;
break;

case 1: e1=calc_err();
pid_cycle_no++;
break;

case 2: e0=calc_err();
pid_cycle_no++;
break;

case 3: e0=calc_err();
e2 = e1 ;
e1 = e0 ;
break;
}
}

