}

void adc_init(void)
{
ADCON0=0;
ADCON1=0b00001110;
ADCON2=0b10101000;

ADIF = 0;

INTCONbits.GIE = 1;

DelayMs(5);
ADCON0 |= 0b010;
}

int read_adc(void)
{
ADIF = 0;
ADCON0bits.ADON = 1;



DelayMs(10);

ADCON0 |= 0b010;
while(!ADIF){}
ADIF = 0;
ADCON0bits.ADON = 0;
ADC_Count = ADRESH;
ADC_Count <<= 8 ;
ADC_Count += ADRESL;
ADC_Count = ADC_Count & 0x03ff;
ADIF = 0;
return ADC_Count;

}

