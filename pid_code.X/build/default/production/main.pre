int Navigate(int key,int statevariable)
{
int i, max,cur_dig;

for(i=0; i<nodigit-1; i++)
max*=10;

if(statevariable > 9)
cur_dig = (statevariable % (cur_inc * 10))/cur_inc;
else
cur_dig = statevariable;


switch(key)
{
case 1:
if(cur_dig < 9)
if((statevariable + cur_inc )<=(states[sw].statemax_val))
statevariable+=cur_inc;


break;

case 2:
if (cur_dig > 0)
if((statevariable- cur_inc)>=(states[sw].statemin_val))
statevariable-=cur_inc;


break;

case 3:
position++;

cur_inc*=10;

if(cur_inc>max)
cur_inc=1;
break;

case 4:
sw=states[sw].enter_state;

cur_inc=1;
position=0xc0;

break;

case 5:
sw=states[sw].escape_state;
cur_inc=1;
position=0xc0;

break;

case 6:
sw = 2;

break;

}
return(statevariable);
}


void lcd_command(unsigned char cmd)
{
LATD1 = 0;
DATA=((cmd>>4) & 0x0f);
LATA1 = DATA & 0x01;
LATA2 = (DATA & 0x02) >> 1;
LATA3 = (DATA & 0x04) >> 2;
LATA4 = (DATA & 0x08) >> 3;
LATA5=1;
DelayMs(10);
LATA5=0;
DATA=(cmd & 0x0f);
LATA1 = DATA & 0x01;
LATA2 = (DATA & 0x02) >> 1;
LATA3 = (DATA & 0x04) >> 2;
LATA4 = (DATA & 0x08) >> 3;
LATA5=1;
DelayMs(10);
LATA5=0;
DelayMs(5);
}

